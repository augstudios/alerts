{
    "name": "augstudios/alerts",
    "description": "Package for sending alerts with Laravel.",
    "keywords": ["laravel", "alerts"],
    "license": "MIT",
    "authors": [
        {
            "name": "Derek J. Augustine",
            "email": "augshow@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0",
        "illuminate/session": "~5.0",
        "myclabs/php-enum": "~1.3"
    },
    "autoload": {
        "psr-4": {
