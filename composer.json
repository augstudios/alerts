        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "mockery/mockery": "0.9.*"
    }
}
